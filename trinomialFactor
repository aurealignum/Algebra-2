package src;

import java.util.Scanner;

public class calc {
	
	Scanner sc = new Scanner(System.in);
	
	public calc (){
		double a = sc.nextInt();
		double b = sc.nextInt();
		double c = sc.nextInt();
		double d = sc.nextInt();
		double x = -10;
		boolean notXInt = true;
		boolean displaying = true;
		double[] xInts;
		int counter = 0;
		int displayCounter = 0;
		xInts = new double[10];
				
		while (notXInt == true){
			
			double y = a * Math.pow(x, 3) + b * Math.pow(x, 2) + c * x + d;
			
			if (y == 0){
				xInts[counter] = x;
				counter++;
			}
			x++;
			
			//stop the loop
			if (x > 100){
				notXInt = false;
			}
		}
		
		while (displaying == true){
			System.out.println(xInts[displayCounter]);
			displayCounter++;
			
			if(displayCounter == xInts.length){
				displaying = false;
			}
		}
		
	}
	
	public static void main(String args[]){
		calc c = new calc(); 
	}
}
